<?xml version="1.0" encoding="UTF-8"?>

<labels xmlns:dc = "http://purl.org/dc/elements/1.1/" xmlns:dct = "http://purl.org/dc/terms/">
	<!-- ==================================================== -->
	<element name="simpledc">
		<label>Метаданные</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:title">
		<label>Название</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:creator">
		<label>Автор</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:subject">
		<label>Тема и ключевые слова</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:description">
		<label>Описание</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:publisher">
		<label>Издатель</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:contributor">
		<label>Участник</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:date">
		<label>Дата</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:type">
		<label>Тип ресурса</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:format">
		<label>Формат</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:identifier">
		<label>Идентификатор ресурса</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:source">
		<label>Источник</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:language">
		<label>Язык</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:relation">
		<label>Relation</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dc:coverage">
		<label>Покрытие</label>
		<description>Например: "Север 90, Юг -90, Восток 180, Запад -180. Глобальное"
		</description>
	</element>
	<!-- ==================================================== -->
	<element name="dc:rights">
		<label>Управление правами</label>
		<description/>
	</element>
	<!-- ==================================================== -->
	<element name="dct:modified">
		<label>Дата измененния</label>
		<description>Дата, когда были изменены метаданные</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:abstract">
		<label>Реферат</label>
		<description>Краткое описание содержимого описываемого ресурса.
</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:spatial">
		<label>Территориально</label>
		<description>Пространственные характеристики содержимого ресурса.</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:audience">
		<label>Целевая аудитория</label>
		<description>Класс лиц, для которых ресурс может быть интересен или полезен.</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:accrualMethod">
		<label>Метод пополнения</label>
		<description>Метод, которым элементы добавляются в коллекцию.</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:accrualPeriodicity">
		<label>Периодичность пополнения</label>
		<description>Частота, с которой элементы добавляются в коллекцию.</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:accrualPolicy">
		<label>Политика пополнения</label>
		<description>Политика руководства по добавлению элементов в коллекцию.</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:instructionalMethod">
		<label>Учебный метод</label>
		<description>Процесс, используемый инженером знаний, навыки и установки,
		разработанные для поддержания ресурса..</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:provenance">
		<label>Происхождение</label>
		<description>Утверждение любых изменений в собственности и пепечении
		ресурса с момента его создания, которые были обозначены для его подлинности,
		целостности и интерпретации.</description>
	</element>
	<!-- ==================================================== -->
	<element name="dct:rightsHolder">
		<label>Правообладатель</label>
		<description>Человек или организация вледеющие или управляющие
		правами над ресурсом.</description>
	</element>
	<!-- ==================================================== -->
</labels>
