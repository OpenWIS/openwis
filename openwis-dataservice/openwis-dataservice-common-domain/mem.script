CREATE MEMORY TABLE OPENWIS_UPDATE_FREQUENCY(REQUEST_OBJECT_TYPE VARCHAR(30) NOT NULL,UPDATE_FREQUENCY_ID BIGINT NOT NULL PRIMARY KEY,RECURRENT_PERIOD INTEGER,RECURRENT_SCALE VARCHAR(255),FROM_DATE TIMESTAMP,TO_DATE TIMESTAMP)
CREATE MEMORY TABLE OPENWIS_VALUE(VALUE_ID BIGINT NOT NULL PRIMARY KEY,VALUE VARCHAR(255))
