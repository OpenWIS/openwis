CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE OPENWIS_BLACKLIST_INFO(BLACKLIST_INFO_ID BIGINT NOT NULL PRIMARY KEY,NB_DISS_BLACKLIST BIGINT NOT NULL,NB_DISS_WARN BIGINT NOT NULL,STATUS VARCHAR(255),USER_ID VARCHAR(255) NOT NULL,VOL_DISS_BLACKLIST BIGINT NOT NULL,VOL_DISS_WARN BIGINT NOT NULL,CONSTRAINT SYS_CT_718 UNIQUE(USER_ID))
CREATE MEMORY TABLE OPENWIS_CACHED_FILE(CACHED_FILE_ID BIGINT NOT NULL PRIMARY KEY,CHECKSUM VARCHAR(255),FILENAME VARCHAR(255),FILESIZE BIGINT,INSERTION_DATE TIMESTAMP,INTERNAL_FILENAME VARCHAR(255),NUMBER_OF_CHECKSUM_BYTES INTEGER,PATH VARCHAR(255),PRIORITY INTEGER,RECEIVED_FROM_GTS BOOLEAN,CONSTRAINT SYS_CT_722 UNIQUE(FILENAME,CHECKSUM))
CREATE INDEX CACHED_FILE_FILENAME_INDEX ON OPENWIS_CACHED_FILE(FILENAME)
CREATE MEMORY TABLE OPENWIS_CACHE_CONFIGURATION(OPENWIS_CONFIGURATION_KEY VARCHAR(255) NOT NULL PRIMARY KEY,OPENWIS_CONFIGURATION_VALUE BIGINT)
